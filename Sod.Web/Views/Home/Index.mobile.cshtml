@using Newtonsoft.Json
@model List<Sod.Web.Models.PersonViewModel>
@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_Layout.mobile.cshtml";
}
<h2>Mobile</h2>
<div class="row">
    <div class="col-md-12">
        <ul>
            @foreach (var p in Model)
            {
                <li>@JsonConvert.SerializeObject(p)</li>
            }
        </ul>
    </div>
</div>